{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.0"
    },
    "pages/*.php": {
      "runtime": "vercel-php@0.5.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/checkout",
      "dest": "/pages/checkout.php"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.php"
    }
  ]
}